<Window x:Class="WpfDemoApp.Events.BubblingEventWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDemoApp.Events"
        mc:Ignorable="d"
        Title="BubblingEventWindow" Height="300" Width="300">
    <StackPanel x:Name="RootST">
        <StackPanel x:Name="InnerST">
            <Button x:Name="Button1" Height="45" Width="300">
                <CheckBox Content="Check" x:Name="Checkbox1"/>
            </Button>
        </StackPanel>
    </StackPanel>
</Window>

    